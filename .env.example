# Copy this to .env and fill in the values
DISCORD_TOKEN=YOUR_DISCORD_BOT_TOKEN

# Lavalink connection (docker-compose uses localhost:2333 by default)
LAVALINK_URI=http://localhost:2333
LAVALINK_PASSWORD=youshallnotpass
LAVALINK_RESUME_KEY=musicbot
LAVALINK_RESUME_TIMEOUT=60

# Optional: If you enable Spotify/Apple plugins in Lavalink, put creds here (used only by Lavalink, not the bot)
SPOTIFY_CLIENT_ID=your_spotify_client_id
SPOTIFY_CLIENT_SECRET=your_spotify_client_secret
APPLE_MUSIC_MEDIA_TOKEN=your_apple_music_media_token
